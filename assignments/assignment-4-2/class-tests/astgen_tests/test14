/* astgen: expressions involving simple function calls and
   arithmetic operators */

int x, y;

int f() {
    x = f() + f() + f();
    x = f() - f() - f();
    x = f() * f() * f();
    x = f() / f() / f();

    x = f() + f() - f() + f() - f();
    x = f() - f() + f() - f() + f();
    x = f() * f() / f() * f() / f();
    x = f() / f() * f() / f() * f();

    x = -f() + -f() - -f() + -f() - -f();
}

int g(int x) {
    y = g(0) + g(1) - f() * g(1) / f();
    y = g(0) - g(1) * f() / g(1) + f();
    y = g(0) * g(1) / f() + g(1) - f();
    y = g(0) / g(1) + f() - g(1) * f();

    y = -g(0) / -g(1) + -f() - -g(1) * -f();
}

int h(int x, int y, int z) {
    x = h(x-1, y+1, z*2);
}

    